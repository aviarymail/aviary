{{!

align
src
alt
maxWidth
borderRadius

 }}

<table
  role='presentation'
  width='100%'
  align='\{{ align }}'
  cellpadding='0'
  cellspacing='0'
  border='0'
>
  <tr>
    <td align='\{{ align }}'>
      <img
        src='\{{src}}'
        alt='\{{alt}}'
        class='inline-block w-full'
        style='max-width: \{{maxWidth}}; border-radius: \{{borderRadius}}'
      />
    </td>
  </tr>
</table>
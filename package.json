{
  "name": "aviary",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "clean": "./scripts/clean.sh",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start": "turbo run start",
    "fns": "yarn workspace fns",
    "web": "yarn workspace web",
    "graphql-server": "yarn workspace graphql-server",
    "cli": "yarn workspace @aviarymail/cli",
    "client": "yarn workspace @aviarymail/client",
    "db": "yarn workspace @aviarymail/db",
    "typestache": "yarn workspace @aviarymail/typestache",
    "services": "yarn workspace @aviarymail/services",
    "gen:gql:web": "graphql-codegen --config codegen.web.yml"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/typed-document-node": "^2.2.2",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@types/lodash": "^4.14.178",
    "graphql": "^16.3.0",
    "prettier": "^2.5.1",
    "rollup": "^2.66.1",
    "rollup-preset-solid": "^1.1.0",
    "turbo": "^1.1.1",
    "type-fest": "^2.11.0",
    "typescript": "^4.5.5",
    "vitest": "^0.2.5"
  },
  "resolutions": {
    "node-fetch": "^2.6.1",
    "solid-start-node": "0.1.0-alpha.55"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "engines": {
    "yarn": ">=3.1.1",
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@3.1.1"
}
